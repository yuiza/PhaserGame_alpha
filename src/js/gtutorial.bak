var game = new Phaser.Game(600, 400, Pharse.AUTO, '', {preload: preload, create: create, update: update});

function preload(){
	game.load.image('sky', 		'assets/sky.png');
	game.load.image('ground', 	'assets/platform.png');
	game.load.image('star', 	'assets/star.png');
	game.spritesheet('dude', 	'assets/dude.png', 32, 48);
}


function create(){
	game.add.sprite(0, 0, 'star');
}

function update(){
}
